import java.util.*;
class l
{
public static void main(String args[]){
Scanner s=new Scanner(System.in);
 System.out.print("Enter the size of array : ");
int n=s.nextInt();
 System.out.print("Enter the elements  of array : ");
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
Arrays.sort(a);
int b= a[n-2];
System.out.print("2nd largest element is : "+b );
}}
