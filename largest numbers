import java.util.*;
class largest
{
public static void main(String args[]){
Scanner s=new Scanner(System.in);
System.out.print("enter n value ");
int n=s.nextInt();
int a[]=new int[n];
System.out.print("enter n  Numbers  ");
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
Arrays.sort(a);
System.out.print( a[n-1]);
}}
